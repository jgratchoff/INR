set grid nopolar
set output '../plot/paper/Latency-frankfurt(RTT~6.8ms).pdf'
set yrange [0:400]
set xrange [0:750]
set term pdf
set style data lines
set datafile separator ","
set title "HTTP/1.1 vs HTTP/2 mean time taken for request and response \n depending on the number of\n clients and size of pages (RTT ~6.8ms(Frankfurt))"
set xlabel "Number of clients"
set ylabel "Mean time for request and response (in ms)"
set style line 1 lt 1 lw 3 pt 3 linecolor rgb "orange"
set style line 2 lt 1 lw 3 pt 3 linecolor rgb "green"
set style line 3 lt 1 lw 3 pt 3 linecolor rgb "blue"
set style line 4 lt 1 lw 3 pt 3  
plot "report_http_urls_large_v2_1426424392.csv" using 1:10 ls 1 smooth sbezier t "1600kB page size HTTP/2", \
	"report_http_urls_small_v2_1426421038.csv" using 1:10 ls 2 smooth sbezier t "20kB page size HTTP/2", \
	"report_http_urls_large_v1_1426436384.csv" using 1:10 ls 3 smooth sbezier t "1600kB page size HTTP/1.1", \
	"report_http_urls_small_v1_1426431907.csv" using 1:10 ls 4 smooth sbezier t "20kB page size HTTP/1.1"




